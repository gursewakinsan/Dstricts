				Grid.ColumnSpan="3"
				Padding="0"
				BackgroundColor="#1B1D24"
				CornerRadius="38,38,0,0"
				HasShadow="False"
				IsClippedToBounds="True">
                <ScrollView VerticalScrollBarVisibility="Never">
                    <Grid ColumnSpacing="0" RowSpacing="0" Margin="0,0,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="6" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="28" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="65" />
                            <ColumnDefinition Width="31" />
                            <ColumnDefinition Width="65" />
                            <ColumnDefinition Width="28" />
                        </Grid.ColumnDefinitions>
                       
                            <Grid RowSpacing="0" ColumnSpacing="0" Grid.Column="0" Grid.ColumnSpan="6">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="406"/>
                                    <RowDefinition Height="52"/>
                                    <RowDefinition Height="36"/>
                                    <RowDefinition Height="7"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="61"/>
                                    <RowDefinition Height="36"/>
                                    <RowDefinition Height="7"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="36"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>

                                    <RowDefinition Height="50"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="28"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="28"/>
                                </Grid.ColumnDefinitions>
                                <controls:CustomFrame
									Grid.Row="0"
									Grid.Column="0"
                                    Grid.ColumnSpan="3"
									Padding="0,40,0,0"
									BackgroundColor="#1B1D24"
									CornerRadius="38,38,0,0"
									HasShadow="False"
									IsClippedToBounds="True">
                                    <Frame.Background>
                                        <LinearGradientBrush EndPoint="0,0.5">
                                            <GradientStop Offset="0.1" Color="#5E5F67" />
                                            <GradientStop Offset="1.0" Color="#1B1D24" />
                                        </LinearGradientBrush>
                                    </Frame.Background>
                                    <Grid RowSpacing="0" ColumnSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                            <RowDefinition Height="9"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="28"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="40"/>
                                            <RowDefinition Height="1"/>
                                            <RowDefinition Height="40"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="28"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="28"/>
                                        </Grid.ColumnDefinitions>
                                        <Label
                                            Grid.Column="1"
											Grid.Row="0"
											FontAttributes="Bold"
											FontFamily="PoppinsBoldFontFamily"
											FontSize="24"
											HorizontalTextAlignment="Start"
											MaxLines="1" 
											Text="{Binding HotelDetails.FirstName}"
											TextColor="White"/>

                                        <Label
											Grid.Row="2"
											Grid.Column="1"
											FontFamily="PoppinsRegularFontFamily"
											FontSize="14" LineHeight="1.2"
											HorizontalTextAlignment="Start"
											Opacity="0.42"
											Text="Dont forget to wear community bracelet and the tag on common areas at all times."
											TextColor="White"
											VerticalTextAlignment="Start" />

                                        <Grid Grid.Row="4" Grid.Column="1" RowSpacing="13" ColumnSpacing="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="82"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="82"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="82"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="75"/>
                                                <RowDefinition Height="20"/>
                                            </Grid.RowDefinitions>
                                            <Frame 
												Grid.Row="0" 
												Grid.Column="0"
												HasShadow="False"
												CornerRadius="10"
												Opacity="0.5"
												BorderColor="White"
												IsClippedToBounds="True"
												BackgroundColor="Transparent"/>

                                            <Label
												Grid.Row="0"
												Grid.Column="0"
												FontFamily="{StaticResource MaterialFontFamily}"
												FontSize="25"
												HorizontalTextAlignment="Center"
												Text="{x:Static fontawesome:DstrictsAppFlatIcons.PhoneOutline}"
												TextColor="White"
												VerticalTextAlignment="Center"/>

                                            <Label
												Grid.Row="1"
												Grid.Column="0"
												FontFamily="PoppinsRegularFontFamily"
												FontSize="12"
												HorizontalTextAlignment="Center"
												Text="Call us"
												TextColor="White"
												VerticalTextAlignment="Center" />

                                            <Frame 
												Grid.Row="0" 
												Grid.Column="2"
												HasShadow="False"
												CornerRadius="10"
												Opacity="0.5"
												BorderColor="White"
												IsClippedToBounds="True"
												BackgroundColor="Transparent"/>

                                            <Label
												Grid.Row="0"
												Grid.Column="2"
												FontFamily="{StaticResource MaterialFontFamily}"
												FontSize="25"
												HorizontalTextAlignment="Center"
												Text="{x:Static fontawesome:DstrictsAppFlatIcons.HomeOutline}"
												TextColor="White"
												VerticalTextAlignment="Center"/>

                                            <Label
												Grid.Row="1"
												Grid.Column="2"
												FontFamily="PoppinsRegularFontFamily"
												FontSize="12"
												HorizontalTextAlignment="Center"
												Text="Order taxi"
												TextColor="White"
												VerticalTextAlignment="Center" />

                                            <Frame 
												Grid.Row="0" 
												Grid.Column="4"
												HasShadow="False"
												CornerRadius="10"
												Opacity="0.5"
												BorderColor="White"
												IsClippedToBounds="True"
												BackgroundColor="Transparent"/>

                                            <Label
												Grid.Row="0"
												Grid.Column="4"
												FontFamily="{StaticResource MaterialFontFamily}"
												FontSize="25"
												HorizontalTextAlignment="Center"
												Text="{x:Static fontawesome:DstrictsAppFlatIcons.HomeOutline}"
												TextColor="White"
												VerticalTextAlignment="Center"/>

                                            <Label
												Grid.Row="1"
												Grid.Column="4"
												FontFamily="PoppinsRegularFontFamily"
												FontSize="12"
												HorizontalTextAlignment="Center"
												Text="Wake-up call"
												TextColor="White"
												VerticalTextAlignment="Center" />
                                        </Grid>

                                        <BoxView
											Grid.Column="1"
											Grid.Row="6"
											BackgroundColor="White"
											HeightRequest="1"
											Opacity="0.1"/>
                                    </Grid>
                                </controls:CustomFrame>

                                <ScrollView
                                    Grid.Row="8"
                                    Grid.Column="1"
                                    Grid.ColumnSpan="2"
                                    Orientation="Horizontal"
                                    HorizontalScrollBarVisibility="Never">
                                    <Grid ColumnSpacing="0">
										<Grid.ColumnDefinitions>
										<ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="278"/>
                                        <ColumnDefinition Width="27"/>
                                        <ColumnDefinition Width="278"/>
                                        <ColumnDefinition Width="28"/>
                                    </Grid.ColumnDefinitions>
                                   
                                        <Frame 
                                            Margin="0,0,27,0"
                                            Grid.Column="0"
                                            Padding="0"
                                            WidthRequest="278"
                                            IsClippedToBounds="True" 
                                            HasShadow="False"
                                            CornerRadius="16"
                                            BackgroundColor="#F40000" IsVisible="{Binding HotelDetails.IsGuestCheckinLeft}">
                                            <StackLayout Spacing="0" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="PLEASE"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsBoldFontFamily"
													FontSize="40"
													Text="CHECK-IN"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="ALL"
													TextColor="White" />

                                                <BoxView
                                                    Margin="0,20,0,60"
                                                    HeightRequest="1"
                                                    WidthRequest="150"
                                                    BackgroundColor="White" 
                                                    HorizontalOptions="CenterAndExpand" 
                                                    VerticalOptions="StartAndExpand"/>

                                                <Button
                                                    CornerRadius="6"
                                                    HeightRequest="50"
                                                    WidthRequest="175"
                                                    Text="{Binding HotelDetails.DisplayGuestCheckinLeft}"
                                                    FontSize="20"
                                                    TextColor="White"
                                                    BackgroundColor="#1B1D24"
                                                    VerticalOptions="EndAndExpand"
                                                    HorizontalOptions="CenterAndExpand"
                                                    FontFamily="PoppinsSemiBoldFontFamily"
													Command="{Binding GoToAdultsAndChildrenPageCommand}" />
                                            </StackLayout>
                                        </Frame>

                                    <Button 
                                        Grid.Column="1"
                                        HeightRequest="406"
                                        WidthRequest="278" 
                                        CornerRadius="16"
                                        BorderWidth="3" BackgroundColor="Transparent"
                                        BorderColor="#0C8CE8"/>

                                   
                                        <StackLayout  Grid.Column="1" Spacing="0" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="PLEASE"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsBoldFontFamily"
													FontSize="40"
													Text="MAKE UP"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="ROOM"
													TextColor="White" />

                                                <BoxView
                                                    Margin="0,20,0,60"
                                                    HeightRequest="1"
                                                    WidthRequest="150"
                                                    BackgroundColor="White" 
                                                    HorizontalOptions="CenterAndExpand" 
                                                    VerticalOptions="StartAndExpand"/>

                                                <Button
                                                    CornerRadius="6"
                                                    HeightRequest="50"
                                                    WidthRequest="175"
                                                    Text="Request"
                                                    FontSize="20"
                                                    TextColor="White"
                                                    BackgroundColor="#0C8CE8"
                                                    VerticalOptions="EndAndExpand"
                                                    HorizontalOptions="CenterAndExpand"
                                                    FontFamily="PoppinsSemiBoldFontFamily"/>
                                            </StackLayout>

                                        <Frame 
                                             Grid.Column="3"
                                            Padding="0"
                                            WidthRequest="278"
                                            IsClippedToBounds="True" 
                                            HasShadow="False"
                                            CornerRadius="16"
                                            BackgroundColor="#0C8CE8">
                                            <StackLayout Spacing="0" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="DO"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsBoldFontFamily"
													FontSize="40"
													Text="NOT"
													TextColor="White" />

                                                <Label
                                                    HorizontalTextAlignment="Center"
													FontFamily="PoppinsRegularFontFamily"
													FontSize="40"
													Text="DISTURB"
													TextColor="White" />

                                                <BoxView
                                                    Margin="0,20,0,60"
                                                    HeightRequest="1"
                                                    WidthRequest="150"
                                                    BackgroundColor="White" 
                                                    HorizontalOptions="CenterAndExpand" 
                                                    VerticalOptions="StartAndExpand"/>

                                                <Button
                                                    CornerRadius="6"
                                                    HeightRequest="50"
                                                    WidthRequest="175"
                                                    Text="Cancel"
                                                    FontSize="20"
                                                    TextColor="White"
                                                    BackgroundColor="#F40000"
                                                    VerticalOptions="EndAndExpand"
                                                    HorizontalOptions="CenterAndExpand"
                                                    FontFamily="PoppinsSemiBoldFontFamily"/>
                                            </StackLayout>
                                        </Frame>
                                    </Grid>
                                </ScrollView>

                                <Label
									Grid.Row="10"
                                    Grid.Column="1"
									FontAttributes="Bold"
									FontFamily="PoppinsBoldFontFamily"
									FontSize="24"
									HorizontalTextAlignment="Start"
									MaxLines="1" 
									Text="Welcome"
									TextColor="White"/>

                                <Label
									Grid.Row="12"
									Grid.Column="1"
									FontFamily="PoppinsRegularFontFamily"
									FontSize="14" LineHeight="1.2"
									HorizontalTextAlignment="Start"
									Opacity="0.42"
									Text="Enjoy guest/tenants perks. Read up and follow the rules.  Always wear the bracelet in common areas"
									TextColor="White"
									VerticalTextAlignment="Start" />

                                <BoxView
									Grid.Column="1"
									Grid.Row="14"
									BackgroundColor="White"
									HeightRequest="1"
									Opacity="0.2"/>

                                <Grid Grid.Row="15" Grid.Column="1" RowSpacing="0" ColumnSpacing="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="13"/>
                                        <RowDefinition Height="44"/>
                                        <RowDefinition Height="13"/>
                                        <RowDefinition Height="1"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="40"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="25"/>
                                    </Grid.ColumnDefinitions>
                                    <Label
										Grid.Row="1"
										Grid.Column="0"
										FontFamily="{StaticResource MaterialFontFamily}"
										FontSize="25"
										HorizontalTextAlignment="Center"
										Text="{x:Static fontawesome:DstrictsAppFlatIcons.Settings}"
										TextColor="#0C8CE8"
										VerticalTextAlignment="Center"/>

                                    <Label
										Grid.Row="1"
										Grid.Column="1"
										FontFamily="PoppinsRegularFontFamily"
										FontSize="16"
										HorizontalTextAlignment="Start"
										Text="Get Wi-fi"
										TextColor="White"
                                        Margin="20,0,0,0"
										VerticalTextAlignment="Center" />

                                    <Label
										Grid.Row="1"
										Grid.Column="2"
										FontFamily="{StaticResource MaterialFontFamily}"
										FontSize="25"
										HorizontalTextAlignment="Center"
										Text="{x:Static fontawesome:DstrictsAppFlatIcons.ChevronRight}"
										TextColor="White"
                                        Opacity="0.7"
										VerticalTextAlignment="Center"/>

                                    <BoxView
										Grid.Column="1"
										Grid.Row="3"
                                        Grid.ColumnSpan="2"
										BackgroundColor="White"
										HeightRequest="1"
										Opacity="0.2" 
                                        Margin="20,0,0,0"/>
                                </Grid>

                                <Grid Grid.Row="16" Grid.Column="1" RowSpacing="0" ColumnSpacing="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="13"/>
                                        <RowDefinition Height="44"/>
                                        <RowDefinition Height="13"/>
                                        <RowDefinition Height="1"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="40"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="25"/>
                                    </Grid.ColumnDefinitions>
                                    <Label
										Grid.Row="1"
										Grid.Column="0"
										FontFamily="{StaticResource MaterialFontFamily}"
										FontSize="25"
										HorizontalTextAlignment="Center"
										Text="{x:Static fontawesome:DstrictsAppFlatIcons.Settings}"
										TextColor="#0C8CE8"
										VerticalTextAlignment="Center"/>

                                    <Label
										Grid.Row="1"
										Grid.Column="1"
										FontFamily="PoppinsRegularFontFamily"
										FontSize="16"
										HorizontalTextAlignment="Start"
										Text="Hotel Rules"
										TextColor="White"
                                        Margin="20,0,0,0"
										VerticalTextAlignment="Center" />

                                    <Label
										Grid.Row="1"
										Grid.Column="2"
										FontFamily="{StaticResource MaterialFontFamily}"
										FontSize="25"
										HorizontalTextAlignment="Center"
										Text="{x:Static fontawesome:DstrictsAppFlatIcons.ChevronRight}"
										TextColor="White"
                                        Opacity="0.7"
										VerticalTextAlignment="Center"/>

                                    <BoxView
										Grid.Column="1"
										Grid.Row="3"
                                        Grid.ColumnSpan="2"
										BackgroundColor="White"
										HeightRequest="1"
										Opacity="0.2" 
                                        Margin="20,0,0,0"/>
                                </Grid>

                                <Label
									Grid.Row="18"
                                    Grid.Column="1"
									FontAttributes="Bold"
									FontFamily="PoppinsBoldFontFamily"
									FontSize="24"
									HorizontalTextAlignment="Start"
									MaxLines="1" 
									Text="Free amenities"
									TextColor="White"/>

                                <Label
									Grid.Row="20"
									Grid.Column="1"
									FontFamily="PoppinsRegularFontFamily"
									FontSize="14" LineHeight="1.2"
									HorizontalTextAlignment="Start"
									Opacity="0.42"
									Text="Easily reach your neighbor, board of directors and security guard from here."
									TextColor="White"
									VerticalTextAlignment="Start" />

                                <ScrollView 
                                    Grid.Row="22"
									Grid.Column="1"
                                    Grid.ColumnSpan="2"
                                    Orientation="Horizontal"
                                    HorizontalScrollBarVisibility="Never">
                                    <StackLayout 
                                        Spacing="0"
                                        Orientation="Horizontal"
                                        HorizontalOptions="StartAndExpand"
                                        BindableLayout.ItemsSource="{Binding GuestServicesList}">
                                        <BindableLayout.ItemTemplate>
                                            <DataTemplate>
                                                <Grid RowSpacing="10" ColumnSpacing="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="76"/>
                                                        <RowDefinition Height="20"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="76"/>
                                                        <ColumnDefinition Width="20"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Button 
                                                        Grid.Row="0" 
                                                        Grid.Column="0"
                                                        BorderWidth="2"
                                                        BorderColor="#0C8CE8"
                                                        HeightRequest="76"
                                                        WidthRequest="76"
                                                        CornerRadius="38"
                                                        FontSize="25"
                                                        TextColor="White"
                                                        Padding="0"
                                                        VerticalOptions="CenterAndExpand"
                                                        HorizontalOptions="StartAndExpand"
